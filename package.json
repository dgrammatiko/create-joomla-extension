{
  "name": "@dgrammatiko/create-joomla-extension",
  "version": "0.0.1-0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Scaffolding for Joomla Extensions",
  "bin": {
    "create-joomla-ext": "./dist/index.js"
  },
  "exports": "./index.mjs",
  "scripts": {
    "prepublishOnly": "np --no-cleanup --yolo --no-publish",
    "start": "node ./index.mjs",
    "bundle": "rollup -c rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgrammatiko/J4-scaffold.git"
  },
  "author": "Dimitris Grammatikogiannis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dgrammatiko/J4-scaffold/issues"
  },
  "type": "module",
  "homepage": "https://github.com/dgrammatiko/J4-scaffold#readme",
  "preferGlobal": true,
  "dependencies": {
    "change-case": "^4.1.2",
    "fs-extra": "^11.1.1",
    "inquirer": "^9.1.5",
    "user-meta": "^1.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "np": "^7.7.0",
    "rollup": "^3.20.2"
  },
  "files": [
    "dist/"
  ]
}
